#include <iostream>
using namespace std;

int main()
{
	char arr[15]={'1','2','3','4','5','6','7','8','9','0','1','2'};
	char *c=arr;
	int num[12];
	int sum=0;
	for(int i=0;i<12;i++,c++)
	{
		if(i<9)
		{
			cout<<"\nThe "<<*c<<" month number is ";
			cin>>num[i];
			sum+=num[i];
		}
		else
		{
			cout<<"\nThe 1"<<*c<<" month number is ";
			cin>>num[i];
			sum+=num[i];
		}
	}
	cout<<"This year num is "<<sum<<endl;
	cin.get();
	return 0;
}
